<% if @ldapize -%>
group: cache [success=return notfound=continue unavail=continue tryagain=continue] files ldap
<%- else -%>
group: cache [success=return notfound=continue unavail=continue tryagain=continue] files
<% end -%>
hosts: cache [success=return notfound=continue unavail=continue tryagain=continue] files dns
netgroup: compat
networks: cache [success=return notfound=continue unavail=continue tryagain=continue] files
<% if @ldapize -%>
passwd: cache [success=return notfound=continue unavail=continue tryagain=continue] files ldap
<%- else -%>
passwd: cache [success=return notfound=continue unavail=continue tryagain=continue] files
<% end -%>
shells: files
services: cache [success=return notfound=continue unavail=continue tryagain=continue] files
protocols: cache [success=return notfound=continue unavail=continue tryagain=continue] files
rpc: cache files
